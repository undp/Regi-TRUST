{
  "name": "vite-react-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon src/server/main.js -w src/server",
    "start": "NODE_ENV=production node src/server/main.js",
    "build": "vite build --base=/ui",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext cjs,jsx --report-unused-disable-directives --max-warnings 0",
    "test": "cross-env NODE_ENV=test jest",
    "preview": "vite preview",
    "license:summarize": "license-checker --summary",
    "license:scan": "license-checker --csv --out THIRD-PARTY.txt"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "memorystore": "^1.6.7",
    "morgan": "^1.10.0",
    "needle": "^3.2.0",
    "openid-client": "^5.6.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "vite-express": "*"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.7.0",
    "license-checker": "^25.0.1",
    "nock": "^13.4.0",
    "nodemon": "^3.0.1",
    "sass": "^1.69.5",
    "supertest": "^6.3.3",
    "vite": "^5.0.2"
  }
}
